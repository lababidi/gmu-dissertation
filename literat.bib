@Article{Singal1977,
  author = 	 {C. M. Singal and T. P. Das},
  title = 	 {Electronic structure of ferromagnetic hcp cobalt. II. Cohesive energy and core exchange splittings$^*$},
  journal = 	 {Phys. Rev. B},
  year = 	 {1977},
  OPTkey = 	 {},
  OPTvolume = 	 {16},
  OPTnumber = 	 {11},
  OPTpages = 	 {5093-5107},
  OPTmonth = 	 {December},
  OPTabstract =  {From the self-consistent unrestricted-Hartree-Fock electronic structure of cobalt metal including pair-correlation effects, the cohesive energy is evaluated to be 0.347 Ry, compared to the experimental value of 0.323 Ry. The correlation among the states in the partially filled 3$d$ band, the Hubband correlation, is found to be the most important contributor to the binding, with significant comtribution from other types of correlation effects involving the core and band electrons. The balance of the cohesive energy arises from one-electron-type effects, primarily from the increased number of 3$d$-like electrons as compared to the atom. This results in an increased attractive interaction of the 3$d$-like electrons with the nucleus, the total kinetic energy from these states being also reduced by the hybridization of the 3$d$ states with the orthogonalized plane waves. This contribution to the cohesive energy from the 3$d$-like and free-like band states is +3.574 Ry which is significantly canceled by a compensating contribution of -3.409 Ry from the change in the energies of the core states. The exchange splitting of the core levels is evaluated and compared with experiment.},
  OPTnote = 	 {},
  OPTannote = 	 {}
}

