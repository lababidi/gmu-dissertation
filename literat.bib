@Article{Rzyman1996,
  author = 	 {K. Rzyman and Z. Moser and R. E. Watson and M. Weinert},
  title = 	 {Enthalpies of formation of Ni$_3$Al: Experiment versus theory},
  journal = 	 {J. Phase Equilib.},
  year = 	 {1996},
  OPTkey = 	 {},
  volume = 	 {17},
  number = 	 {3},
  pages = 	 {173-178},
  month = 	 {June},
  note = 	 {Using Al solution calorimetry, enthalpies of formation of Ni$_3$Al in the L1$_2$ structure ranging from -41.3$\pm$1.3 to -42.3 kJ/mol were determined at temperatures from 300 to 1123 K. These enthalpies are substantial for an intermetallic compound and display a slight temperature dependence, which is contained within the experimental error. This temperature dependence is related to the lack of any transformations of the compound, which remains ordered up to the melting temperature. The measured enthalpies were combined with other thermodynamic data to estimate the excess entropy of formation of Ni3Al. The high degree of ordering in the compound is reflected in the large negative value of the excess entropy, whose absolute value exceeds those for other high melting temperature intermetallic compounds. The full-potential linearized augmented Slater-type orbital method (FLASTO) was used to calculate the enthalpies of formation of paramagnetic and ferromagnetic Ni$_3$Al. These enthalpies of formation indicate the compound to be a weak ferromagnet, and they are in good agreement with the calorimetric data.},
  OPTannote = 	 {}
}

@Article{Wang2004,
