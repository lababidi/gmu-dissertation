  title = 	 {Thermodynamic properties of Al, Ni, NiAl, and Ni$_3$Al from first-principles calculations},
  journal = 	 {Acta Materi.},
  year = 	 {2004},
  key = 	 {Nickel alloys; Aluminium alloys; Ab initio electron theory; Thermodynamics},
  volume = 	 {52},
  OPTnumber = 	 {},
  pages = 	 {2665-2671},
  month = 	 {March},
  note = 	 {The 0-K total energies are calculated using the ab initio plane wave pseudopotential method within the generalized gradient approximation. Our calculations show that the enthalpies of formation are slightly temperature dependent with a slope of -1.6 J/mol/K for NiAl and -1.2 J/mol/K for Ni$_3$Al. At room temperature, our calculated enthalpy of formation for NiAl is -62.7 kJ/mol (-0.6498 eV) which is 5\% higher than the recently calorimetric value of -66.1 kJ/mol (-0.6851 eV) by Rzyman et al. For Ni_3Al, our calculated enthalpy of formation at 300K is -41.1 kJ/mol (-0.4260 eV) which is extremely close to the calorimetric value of -41.3 kJ/mol (-0.4280 eV) by Rzyman et al.},
  OPTannote = 	 {}
}

@Article{Clapp1973,
