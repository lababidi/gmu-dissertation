@Article{Becquart1993,
  author = 	 {C. S. Becquart and P. C. Clapp and J. A. Rifkin},
  title = 	 {Molecular-dynamics simulation of tweed structure and the $\omega$ phase in Ni-Al},
  journal = 	 {Phys. Rev. B},
  year = 	 {1993},
  OPTkey = 	 {},
  volume = 	 {48},
  number = 	 {1},
  pages = 	 {6-13},
  month = 	 {July},
  OPTnote = 	 {The occurrence of tweed structure and of the $\omega$ phase has been studied in Ni$_{62.5}$Al$_{37.5}$. Tweed is a mottled structure that is observed prior to transformations such as martensitic transformations. In some cases, it has been found to accompany $\omega$-phase formation. The simulations were performed on two different types of Ni$_{62.5}$Al$_{37.5}$, one ordered and one disordered, each containing 1024 atoms (8$\times$8$\times$8 bcc unit cells) with a lattice parameter of 2.88\AA. In a stoichiometric Ni-Al B2 structure, Al atoms were replaced by the extra Ni atoms at random (disordered array) and in a regular ordered pattern (ordered array) in order to achieve 62.5-37.5 composition without any change in the type of cell. There are many ways of randomly distributing the ``extra'' Ni atoms on the Al sublattice, and to study the influence of this factor a run was performed at 10 K for 3000 steps (90ps) for two configurationally different disordered B2 arrays. The simulations were based upon EAM potentials developed by Voter and Chen. In the disordered B2 array, diffuse streakings were observed mainly along the $\langle 110\rangle$ directions but some also along the $\langle 112\rangle$ direction. The $\langle 112\rangle$ streaking is usually associated with the formation of the $\omega$ phase. However, with the ordered array, virtually no streaking appeared, strongly suggesting that heterogeneous defects (such as compositional disorder) are necessary to induce both tweed structure and the $\omega$ phase. Again $\langle 110\rangle\{100\}$ shears were noticed in the disordered lattice.},
  OPTannote = 	 {}
}

